{
  "name": "nordhealth-interview",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "supabase:init": "supabase init",
    "supabase:login": "supabase login",
    "supabase:link": "dotenv -- cross-var supabase link --project-ref $SUPABASE_PROJECT_REF",
    "db:migrate:new": "supabase migration new",
    "db:reset": "supabase db reset --linked",
    "db:seed": "node --env-file=.env database/seed.js",
    "supabase:types": "dotenv -- cross-var npx supabase gen types --lang=typescript --project-id $SUPABASE_PROJECT_REF --schema public > database/types.ts"
  },
  "dependencies": {
    "@provetcloud/css": "^1.1.0",
    "@provetcloud/web-components": "^1.3.2",
    "@supabase/supabase-js": "^2.48.1",
    "@vueuse/core": "^12.7.0",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^8.0.0",
    "pinia": "^2.3.1",
    "supabase": "^2.12.1",
    "vue": "^3.5.13",
    "vue-query": "^1.26.0",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.13.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.3.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.18.0",
    "eslint-plugin-vue": "^9.32.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.4.2",
    "typescript": "~5.7.3",
    "unplugin-auto-import": "^0.17.1",
    "unplugin-vue-components": "^28.1.0",
    "unplugin-vue-router": "^0.11.2",
    "vite": "^6.0.11",
    "vite-plugin-vue-devtools": "^7.7.1",
    "vue-tsc": "^2.2.0"
  }
}
